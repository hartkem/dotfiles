# alias kgap="kubectl get po -A --sort-by=.metadata.namespace"
kgap() {
  # Base command to get all pods sorted by namespace
  local base_cmd="kubectl get po -A --sort-by=.metadata.namespace"

  # Check if an argument is provided
  if [ -z "$1" ]; then
    # No argument, just run the base command
    eval "$base_cmd"
  else
    # Argument provided, filter the output using grep
    eval "$base_cmd" | grep "$1"
  fi
}


